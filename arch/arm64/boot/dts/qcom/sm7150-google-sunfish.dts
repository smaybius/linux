&tlmm {
	gpio-reserved-ranges = <59 4>;
	
	drv2624_rst: drv2624_rst {
		mux {
			pins = "gpio11";
			function = "gpio";
		};
		config {
			pins = "gpio11";
			drive-strength = <2>;
			output-low;
			bias-disable;
		};
	};

	rt5514_handshake_ack: rt5514-handshake-ack-state {
		pins = "gpio30";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-down;
		input-enable;
	};

	rt5514_reset: rt5514-reset-state {
		pins = "gpio33";
		function = "gpio";
		drive-strength = <2>;
		output-high;
	};

	rt5514_handshake: rt5514-handshake-state {
		pins = "gpio57";
		function = "gpio";
		drive-strength = <2>;
		output-low;
	};

	rt5514_irq: rt5514-irq-state {
		pins = "gpio58";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-down;
		input-enable;
	};

	touch_irq_default: touch_irq_default {
		mux {
			pins = "gpio9";
			function = "gpio";
		};

		config {
			pins = "gpio9";
			drive-strength = <2>;
			bias-disable;
		};
	};

	touch_reset_default: touch_reset_default {
		mux {
			pins = "gpio8";
			function = "gpio";
		};

		config {
			pins = "gpio8";
			drive-strength = <2>;
			output-high;
			bias-disable;
		};
	};

	touch_switch_default: touch_switch_default {
		mux {
			pins = "gpio72";
			function = "gpio";
		};

		config {
			pins = "gpio72";
			drive-strength = <2>;
			output-low;
			bias-disable;
		};
	};

	qup_uart3_sleep: qup-uart3-sleep-state {
		cts-pins {
			pins = "gpio38";
			function = "gpio";
			bias-pull-down;
		};

		rts-pins {
			pins = "gpio39";
			function = "gpio";
			bias-pull-down;
		};

		tx-pins {
			pins = "gpio40";
			function = "gpio";
			bias-pull-up;
		};

		rx-pins {
			pins = "gpio41";
			function = "gpio";
			bias-pull-up;
		};
	};
};

